{
  "name": "deta-micros-typescript",
  "version": "1.0.0",
  "description": "Deta micros",
  "main": "index.js",
  "scripts": {
    "dev:micros": "tsup src/index.ts --watch --platform=node --format=cjs --out-dir micros/dist --onSuccess=\"node micros/dev.js\"",
    "build:micros": "tsup src/index.ts --minify --platform=node --format=cjs --out-dir micros/dist",
    "deploy:micros": "pnpm run build:micros && cp package.json micros/package.json && cd micros && deta deploy"
  },
  "keywords": ["deta", "micros"],
  "author": "Aris Riswanto",
  "repository": {
    "url": "https://github.com/arisris/deta-micros-typescript"
  },
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "tsup": "^6.2.3"
  }
}